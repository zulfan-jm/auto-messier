<?xml version="1.0"?>
<launch>
    <node pkg="move_base" type="move_base" name="move_base">
        <rosparam file="$(find rovid_1)/params/common_costmap_params.yaml" command="load" ns="global_costmap"/>
        <rosparam file="$(find rovid_1)/params/common_costmap_params.yaml" command="load" ns="local_costmap"/>
        <rosparam file="$(find rovid_1)/params/dwa_local_planner_params.yaml" command="load"/>
        <!-- <rosparam file="$(find rovid_1)/params/teb_local_planner_params.yaml" command="load"/> -->
        <rosparam file="$(find rovid_1)/params/base_global_planner_params.yaml" command="load"/>
        <rosparam file="$(find rovid_1)/params/local_costmap_params.yaml" command="load"/>
        <rosparam file="$(find rovid_1)/params/global_costmap_params.yaml" command="load"/>

        <param name="base_local_planner" value="dwa_local_planner/DWAPlannerROS"/>
        <!-- <param name="base_local_planner" value="teb_local_planner/TebLocalPlannerROS"/> -->
        <param name="base_global_planner" value="global_planner/GlobalPlanner"/>
        <param name="controller_patience" value="20"/>
        <param name="recovery_behavior_enabled" value="false"/>
        <param name="clearing_rotation_allowed" value="false"/>
    </node>
</launch>